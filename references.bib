@article{el-hameed_anodic_2017,
  title = {Anodic {{Coating Characteristics}} of {{Different Aluminum Alloys}} for {{Spacecraft Materials Applications}}},
  author = {{El-Hameed}, Afaf M. Abd and {Abdel-Aziz}, Yehia A. and {El-Tokhy}, Fatma S.},
